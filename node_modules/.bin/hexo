#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yinzi/aod321.github.io/node_modules/.pnpm/hexo@8.1.1_chokidar@3.6.0/node_modules/hexo/bin/node_modules:/Users/yinzi/aod321.github.io/node_modules/.pnpm/hexo@8.1.1_chokidar@3.6.0/node_modules/hexo/node_modules:/Users/yinzi/aod321.github.io/node_modules/.pnpm/hexo@8.1.1_chokidar@3.6.0/node_modules:/Users/yinzi/aod321.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yinzi/aod321.github.io/node_modules/.pnpm/hexo@8.1.1_chokidar@3.6.0/node_modules/hexo/bin/node_modules:/Users/yinzi/aod321.github.io/node_modules/.pnpm/hexo@8.1.1_chokidar@3.6.0/node_modules/hexo/node_modules:/Users/yinzi/aod321.github.io/node_modules/.pnpm/hexo@8.1.1_chokidar@3.6.0/node_modules:/Users/yinzi/aod321.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/hexo@8.1.1_chokidar@3.6.0/node_modules/hexo/bin/hexo" "$@"
else
  exec node  "$basedir/../.pnpm/hexo@8.1.1_chokidar@3.6.0/node_modules/hexo/bin/hexo" "$@"
fi
